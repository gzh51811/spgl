                                    <input type="text" onkeyup="value=value.replace(/[^\d.]/g,'')" class="form-control" id="indextjsp_mm" placeholder="">
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="exampleInputEmail1">库存</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" onkeyup="this.value=this.value.replace(/\D/g,'')" id="indextjsp_mm" placeholder="">
                                    <span class="input-group-addon">件</span>
                                </div>
                            </div>
                        </div>
                    </div>

